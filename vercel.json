{
    "version": 2,
    "builds": [
      {
        "src": "tsconfig.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "dist/index.js"
      },
      {
        "src": ".*",
        "dest": "dist/index.js"
      }
    ],
    "env": {
      "PORT": "src/config.PORT",
      "SECRET_REGISTRATION_CODE": "src/config.SECRET_REGISTRATION_CODE",
      "SECRET_TOKEN": "src/config.SECRET_TOKEN",
      "URI_DB": "src/config.URI_DB"
    }
  }
  
